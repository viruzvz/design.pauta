doctype html
html(lang='pt')
  style(role='Apenas para navegacao do prototipo, corresponde ao menu-item').
    #btn-navbar--calendar{
      display:none;
    }
  - var rand = function () { return Math.random().toString(36).replace(/[^a-z]+/g, '').substr(0, 5) }
  include jade/head

  body#sfcss
    aside
      include jade/sidebar
    main
      header
        include jade/navbar
        .col-md-12
          ol.breadcrumb.breadcrumb-sf.milli.u-spacer--xl
            li
              a(href='paginaInicial.html') Página inicial
            li.active Divulgação Externa
      article.col-md-12
        include jade/paginas/divulgacaoExterna

  include jade/footer




